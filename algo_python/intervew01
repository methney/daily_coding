# you can write to stdout for debugging purposes, e.g.
# print("this is a debug message")

def solution(A):
    aa = min(A)
    if aa <= 0 :
        aa = 0
    return get_min_in_arr(A,aa)

def get_min_in_arr(A, i):
    if i+1 in A :
        return get_min_in_arr(A,i+1)
    else : 
        return i + 1



A = [1,3,-6,4,1,2]

print(solution(A))
    
